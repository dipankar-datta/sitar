    "url": "https://github.com/dipankar-datta/sitar/issues"
  },
  "homepage": "https://github.com/dipankar-datta/sitar#readme",
  "devDependencies": {
    "@types/jest": "^26.0.20",
    "@types/lodash": "^4.14.168",
    "@types/object-hash": "^1.3.4",
    "@types/uuid": "^8.3.0",
    "codecov": "^3.1.0",
